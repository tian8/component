
<import src="../../components/cpbackground.wxml" />
<template is="background" data="{{...wxui.background}}" />
<view class="page">
    <view class="page_hd">
        <view class="page_title">背景幕</view>
        <view class="page_desc">background</view>
    </view>
    <view   class="page_bd">
        <view class="weui-btn-area ezpop" >
            <button class="weui-btn" type="default" bindtap="retain">锁定背景幕</button>
            <button class="weui-btn" type="primary" bindtap="release">释放背景幕</button>
        </view>
    </view>
    <view class="text-center">背景幕锁:{{lock}}</view>
</view>
