<!--pages/dialog/index.wxml-->
<import src="../../components/dialog.wxml" />
<template is="dialog" data="{{...wxui.dialog}}" />
<view class="page">
    <view class="page_hd">
        <view class="page_title">弹出框</view>
        <view class="page_desc">dialog</view>
    </view>
    <view   class="page_bd">
        <view class="weui-btn-area" >
            <button class="weui-btn" type="default" bindtap="showChange">选择弹出框</button>
            <button class="weui-btn" type="primary" bindtap="showMessage">消息弹出框</button>
        </view>
    </view>
</view>